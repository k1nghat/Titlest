{
	"manifest_version": 2,
	"name": "test-icons-trash-too",
	"homepage_url": "http://localhost/",
	"description": "A Vue Browser Extension",
	"version": "0.1.0",
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"permissions": ["activeTab", "tabs", "<all_urls>", "*://*/*", "storage", "contextMenus", "notifications"],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "popup/popup.html",
		"default_title": "CLICK ME!",
		"default_icon": {
			"19": "icons/19.png",
			"38": "icons/38.png"
		}
	},
	"content_scripts": [
		{
			"js": ["content-scripts/content-script.js"],
			"matches": ["<all_urls>"],
			"run_at": "document_start"
		}
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
